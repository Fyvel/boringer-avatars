{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-ts-starter",
  "description": "Vue2 TypeScript Startar project for Vite.",
  "version": "1.0.5",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build": "run-p build-only",
    "build:analyze": "vite build --mode analyze",
    "build-only": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^2.7.14",
    "@boringer-avatars/vue2": "workspace:*",
    "@demo/vue2": "workspace:*",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue2": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "rollup-plugin-visualizer": "^5.8.3",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-checker": "^0.5.1",
    "vitest": "^0.25.6",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.0.12",
    "vue-router": "^3.6.5",
    "vuex": "^4.1.0",
    "vuex-persist": "^3.1.3",
    "deepmerge": "*",
    "@logue/vue2-helpers": "*"
  }
}
