{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@demo/{{target}}",
  "version": "{{version}}",
  "license": "MIT",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/demo.{{target}}.umd.js",
  "module": "./dist/demo.{{target}}.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/demo.{{target}}.es.js",
      "require": "./dist/demo.{{target}}.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.1.3",
    "@vitejs/plugin-vue2": "^2.1.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-checker": "^0.5.1",
    "vue": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.0.12"
  }
}
